type Query {
    users: [User!]! @paginate(defaultCount: 10)
    user(id: ID! @eq): User @find
}

type User {
    id: ID!
    name: String!
    keptRecord: [Record!]
}

type Record {
    id: ID!
    user_id: int!
    year: int!
    month: int!
    income: int!
    outgo_cre: int!
    recordedBy: User!
}
