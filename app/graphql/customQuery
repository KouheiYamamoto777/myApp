type Query {
    users: [User!]! @paginate(defaultCount: 10)
    user(id: ID! @eq): User @find
}

type User {
    id: ID!
    name: String!
    keptRecord: [Record!]
}

type Record {
    id: ID!
    user_id: int!
    year: int!
    month: int!
    income: int!
    outgo_cre: int!
    recordedBy: User!
}


------------


    incomes: [Income!] @hasMany
    posts: [Post!] @hasMany

type Income {
    id: ID!
    year: int!
    month: int!
    income: int!
    outgo_cre: int!
    recordedBy: User!
    user: User @belongsTo
}

type Post {
    id: ID!
    title: String!
    body: String!
    published: Boolean!
    year: int!
    month: int!
    user: User @belongsTo
    comment: Comment @hasMany
}

type Comment {
    id: ID!
    comment: String!
    post: Post @belongsTo
}
